<div class="mt-2">
    <h4>{{category}} - {{workout_date}}</h4>
    <button class="complete-workout btn bye-btn" data-id="{{id}}" data-complete="{{completed}}"><i
            class="fas fa-check"></i> Workout Completed</button>

    <p class="mt-1">
        <a class="btn" data-toggle="collapse" href="#collapseDetails" role="button" aria-expanded="false"
            aria-controls="collapseDetails">
            View Details
        </a>
    </p>
    <div class="collapse" id="collapseDetails">
        <div class="card card-body">
            <p>Distance: {{distance}}</p>
            <p>Duration: {{duration}}</p>
            <p>Workout Details: {{details}}</p>
        </div>
    </div>

</div>